/* IE9, IE10 */
@media screen and (min-width:0\0) {
	.printWrap .profile {border: 5px solid #edfaf9;}
	.printWrap .profile .bgImg {display: none;}
	.printWrap .profile .profCon {color: #111 !important;}
	.printWrap .profile .profCon .right .icon img {border: 2px solid #edfaf9; border-radius: 50%;}
}
/* IE11 */
@media all and (-ms-high-contrast:none){
	*::-ms-backdrop, .printWrap .profile {border: 5px solid #edfaf9;}
	*::-ms-backdrop, .printWrap .profile .bgImg {display: none;}
	*::-ms-backdrop, .printWrap .profile .profCon * {color: #111 !important;}
	*::-ms-backdrop, .printWrap .profile .profCon .right .icon img {border: 2px solid #edfaf9; border-radius: 50%;}
}
